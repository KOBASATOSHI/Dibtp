<% provide(:path, to_do_groups_path ) %>

<h2>ToDo Groups</h2>
<div class='row'>
  <div class='col-xs-4'>
    <%=render 'form' %>
  </div>
  <div class='col-xs-8'>
    <table class='table' id='myTable'>
      <thead>
        <td>名前</td>
        <td>最終更新日</td>
        <td>アクション</td>
      </thead>
      <tbody>
        <%= render @to_do_groups %>
      </tbody>
    </table>
  </div>
</div>
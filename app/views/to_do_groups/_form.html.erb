  <%= form_for @to_do_group , url: yield(:path) do |f|%>
   
    <%= f.label :name %>
    <%= f.text_field :name, id: :name, class:'form-control' %>
    
    <%= f.label :detail %>
    <%= f.text_area :detail, id: :detail, class:'form-control' %>
    
    <%= f.submit 'submit', class:'btn btn-primary' %>
  <% end %>